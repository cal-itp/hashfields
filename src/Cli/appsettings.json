{
    "DataOptions": {
        "Delimiter": ",",
        "Drop": [
            "column1",
            "column3"
        ],
        "HashAlgorithm": "sha256",
        "HyphenateHashes": "false",
        "LowercaseHashes": "true",
        "Skip": [
            "column2"
        ]
    },
    "StreamOptions": {
        "Input": {
            "Path": "./samples/data.csv",
            "Type": "File"
        },
        "Output": {
            "Path": "./samples/data.hashed.csv",
            "Type": "File"
        }
    }
}
